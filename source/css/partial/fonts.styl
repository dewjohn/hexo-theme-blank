@font-face {
    font-family: 'glyph-correction';
    font-display: swap;
    src: url('https://cdn.jsdelivr.net/gh/a2396837/CDN@latest/fonts/glyph-correction.woff2') format('woff2'),
         url('https://cdn.jsdelivr.net/gh/a2396837/CDN@latest/fonts/glyph-correction.woff') format('woff'),
         url('https://cdn.jsdelivr.net/gh/a2396837/CDN@latest/fonts/glyph-correction.ttf') format('truetype'),
         url('https://cdn.jsdelivr.net/gh/a2396837/CDN@latest/fonts/glyph-correction.eot') format('embedded-opentype');
    font-weight: inherit;
    font-style: inherit;
}
